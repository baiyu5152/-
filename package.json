{
  "name": "simmusic",
  "version": "1.0.1",
  "description": "Music",
  "main": "main.js",
  "scripts": {
    "start": "nodemon --watch main.js --exec electron .",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/baiyu5152/-",
  "keywords": [
    "tutorial",
    "demo"
  ],
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "http://localhost:8899/"
      }
    ],
    "directories": {
      "output": "release",
      "app": "./"
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": true
    }
  },
  "author": "Colley.Bai",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^8.0.1",
    "electron-builder": "^22.3.2",
    "electron-packager": "^14.2.1",
    "electron-updater": "^4.2.2",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "electron-store": "^3.2.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0",
    "uuid": "^3.4.0"
  }
}
